<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat code coverage">
				<Configuration>
					<Format>json,cobertura,lcov,teamcity,opencover</Format>
					<Exclude>[coverlet.*.tests?]*,[*]Coverlet.Core*</Exclude>
					<!-- [Assembly-Filter]Type-Filter -->
					<Include></Include>
					<!-- [Assembly-Filter]Type-Filter -->
					<ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
					<ExcludeByFile></ExcludeByFile>
					<!-- Globbing filter -->
					<IncludeDirectory></IncludeDirectory>
					<SingleHit>false</SingleHit>
					<UseSourceLink>true</UseSourceLink>
					<IncludeTestAssembly>false</IncludeTestAssembly>
					<SkipAutoProps>true</SkipAutoProps>
					<DeterministicReport>false</DeterministicReport>
					<CodeCoverage>
						<ModulePaths>
							<Exclude>
								<ModulePath>rfmusb.net.unittests.dll</ModulePath>
							</Exclude>
						</ModulePaths>
					</CodeCoverage>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
	<TestRunParameters>
		<Parameter name="Rfm69ComPort" value="COM6" />
		<Parameter name="Rfm9xComPort" value="COM3" />
		<Parameter name="BaudRate" value="230400" />
	</TestRunParameters>
</RunSettings>