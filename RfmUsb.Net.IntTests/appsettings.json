{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Trace", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Trace"
      },
      {
        "Name": "Console"
      }
    ]
  }
}
