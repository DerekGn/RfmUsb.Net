{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "DeviceConfigurationOptions": {
    "BaudRate": 230400,
    "Rfm69ComPort": "COM6",
    "Rfm9xComPort": "COM3"
  }
}
